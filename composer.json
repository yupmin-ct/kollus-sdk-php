{
  "name": "yupmin-ct/kollus-sdk",
  "description": "Kollus SDK for PHP",
  "minimum-stability": "stable",
  "license": "MIT",
  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "^6.2",
    "symfony/http-foundation": "^3.2",
    "firebase/php-jwt": "^4.0",
    "fabpot/goutte": "^3.2"
  },
  "repositories": {
    "packagist": { "url": "https://packagist.org", "type": "composer" }
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "mikey179/vfsStream": "^1.6",
    "squizlabs/php_codesniffer": "^2.7",
    "symfony/yaml": "^3.2",
    "satooshi/php-coveralls": "@dev"
  },
  "authors": [
    {
      "name": "yun young-jin",
      "email": "yupmin@catenoid.net"
    }
  ],
  "autoload": {
    "psr-4": {
      "Kollus\\Component\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kollus\\Test\\": "tests/"
    }
  }
}
